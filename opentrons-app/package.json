{
    "name": "irobolab",
    "version": "0.1.0",
    "private": true,
    "main": "main.js",
    "dependencies": {
        "@testing-library/jest-dom": "^6.4.8",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.4.2",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "chroma-js": "^3.1.1",
        "react": "^18.3.1",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.3.1",
        "react-scripts": "^5.0.1",
        "web-vitals": "^4.2.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "electron-build": "npm run build && electron .",
        "dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 npm run electron\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/chroma-js": "^2.4.4",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "electron": "^32.0.2",
        "typescript": "^4.9.5",
        "wait-on": "^7.0.1"
    }
}
