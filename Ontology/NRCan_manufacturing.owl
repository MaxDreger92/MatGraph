<owl:Class rdf:about="#Annealing">
  <rdfs:subClassOf rdf:resource="#Manufacturing"/>
  <alternative_labels rdf:datatype="http://www.w3.org/2001/XMLSchema#string">['Heat Treatment', 'Thermal Treatment', 'Metal Softening', 'Material Toughening', 'Recrystallization Process']</alternative_labels>
  <description_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A heat treatment process used to alter the physical and sometimes chemical properties of a material to increase its ductility and reduce its hardness.</description_name>
  <onto_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Annealing</onto_name>
</owl:Class>

