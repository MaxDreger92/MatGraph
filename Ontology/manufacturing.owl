  <alternative_labels rdf:datatype="http://www.w3.org/2001/XMLSchema#string">['Material Characterization', 'Property Quantification', 'Materials Testing', 'Physical Property Analysis', 'Material Property Measurement']</alternative_labels>
</owl:Class>

<owl:Class rdf:about="#BETMethod">
